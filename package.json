{
  "name": "@cosva-lab/api-commons",
  "version": "1.2.0",
  "description": "TypeScript NPM Module Boilerplate",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosva-lab/cosva-api-commons.git"
  },
  "keywords": [
    "boilerplate",
    "typescript",
    "npm",
    "module"
  ],
  "author": "Eduard Castellanos",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cosva-lab/cosva-api-commons/issues"
  },
  "homepage": "https://github.com/cosva-lab/cosva-api-commons#readme",
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.873.0",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "ts-jest": "29.2.5",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "@aws-sdk/client-secrets-manager": "^3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.11.0"
  }
}
